# Useful Navigation
alias ..='cd ..'
alias ...='cd ../..'
alias gopen='gnome-open'
alias dirs="ls -1d */"
alias files="ls -p | grep -v /"
alias ll='ls -alF'
alias la='ls -AF'
alias l='ls -l'
alias cl='clear'
alias agrep='la | grep'